# RUN: yaml2obj %s > %t
# RUN: not llvm-elfabi --elf %t --emit-tbe=%t.tbe 2>&1 | FileCheck %s

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_DYN
  Machine:         EM_X86_64
Sections:
  - Name:            .dynstr
    Type:            SHT_STRTAB
    Flags:           [ SHF_ALLOC ]
    Address:         0x1000
#                     \0 b a z\0 s o m e l i b . s o z z z
    Content:         "0062617a00736f6d656c69622e736f7a7a7a"
  - Name:            .dynamic
    Type:            SHT_DYNAMIC
    Flags:           [ SHF_ALLOC ]
    Address:         0x1018
    AddressAlign:    8
    Content:         "0e0000000000000005000000000000000a000000000000000f00000000000000050000000000000000100000000000000600000000000000001000000000000000000000000000000000000000000000"
      # DT_SONAME     5 (0x05)
      # DT_STRSZ      15 (0x0F)
      # DT_STRTAB     0x1000
      # DT_SYMTAB     0x1000
      # DT_NULL       0x0
    Size:            80
    Link:            .dynstr
ProgramHeaders:
  - Type: PT_LOAD
    Flags: [ PF_R ]
    VAddr: 0x1000
    PAddr: 0x1000
    Align: 8
    Sections:
      - Section: .dynstr
      - Section: .dynamic
  - Type: PT_DYNAMIC
    Flags: [ PF_X, PF_R ]
    VAddr: 0x1018
    PAddr: 0x1018
    Sections:
      - Section: .dynamic

# CHECK: String overran bounds of string table (no null terminator) when reading DT_SONAME
