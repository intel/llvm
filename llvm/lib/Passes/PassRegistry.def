                        "LoopExtractorPass",
                        [](bool Single) {
                          if (Single)
                            return LoopExtractorPass(1);
                          return LoopExtractorPass();
                        },
                        parseLoopExtractorPassOptions,
                        "single")
MODULE_PASS_WITH_PARAMS("hwasan",
                        "HWAddressSanitizerPass",
